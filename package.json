{
  "name": "@nestjs/cli",
  "version": "5.5.0",
  "description": "Nest CLI",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "bin": {
    "nest": "bin/nest.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/tslint/bin/tslint -p .",
    "start": "node bin/nest.js",
    "test": "jest --config test/jest-config.json",
    "test:dev": "jest --config test/jest-config.json --watchAll",
    "postinstall": "opencollective"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjs/nest-cli.git"
  },
  "author": "ThomRick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nestjs/nest-cli/issues"
  },
  "homepage": "https://github.com/nestjs/nest-cli#readme",
  "dependencies": {
    "@angular-devkit/core": "^0.8.1",
    "@angular-devkit/schematics-cli": "^0.1102.19",
    "@nestjs/schematics": "^5.8.0",
    "@nuxtjs/opencollective": "^0.1.0",
    "@types/jest": "^22.2.3",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "inquirer": "^9.0.0",
    "node-emoji": "^1.8.1",
    "ora": "^6.0.1",
    "os-name": "^3.0.0",
    "ts-node": "^8.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.41",
    "@types/node": "^9.6.19",
    "@types/node-emoji": "^1.8.0",
    "@types/ora": "^1.3.4",
    "@types/os-name": "^2.0.0",
    "jest": "^22.4.4",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/nest",
    "donation": {
      "text": "Become a partner:"
    }
  }
}
